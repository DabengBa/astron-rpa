[MASTER]
ignore=__pycache__
jobs=0

[MESSAGES CONTROL]
disable=
    too-few-public-methods,
    import-outside-toplevel

[REPORTS]
score=yes

[FORMAT]
max-line-length=120

[DESIGN]
max-branches=16
max-statements=60

[IMPORTS]
allow-wildcard-with-all=no

[BASIC]
good-names=i,j,k,ex,_,id

[EXCEPTIONS]
overgeneral-exceptions=Exception
